{
  "name": "drools-kie-client",
  "version": "0.2.0",
  "description": "Node.js client for Drools KIE server API.",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*-test.js",
    "lint": "eslint index.js test/*.js",
    "prepublish": "nsp check",
    "format": "semistandard",
    "coverage": "./node_modules/.bin/istanbul cover tape -- test/**.js",
    "docs": "./node_modules/.bin/jsdoc -d docs -t ./node_modules/ink-docstrap/template -R README.md index.js"
  },
  "files": [
    "index.js",
    "test",
    "doc"
  ],
  "keywords": [
    "Drools",
    "KIE",
    "REST",
    "Client"
  ],
  "author": "Red Hat, Inc.",
  "engines": {
    "node": ">= 4.0.0"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/bucharest-gold/drools-kie-client#readme",
  "repository": {
    "type": "git",
    "url": "bucharest-gold/drools-kie-client"
  },
  "bugs": "https://github.com/bucharest-gold/drools-kie-client/issues",
  "devDependencies": {
    "eslint": "^3.3.1",
    "eslint-config-semistandard": "^7.0.0-beta.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "ink-docstrap": "^1.2.1",
    "istanbul": "^0.4.3",
    "jsdoc": "^3.4.0",
    "nsp": "^2.6.1",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "roi": "^0.9.0"
  }
}
